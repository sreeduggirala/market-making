# Kraken API Credentials
# Obtain from: https://www.kraken.com/u/security/api
# Note: API secret is base64-encoded
KRAKEN_API_KEY=your_kraken_api_key_here
KRAKEN_API_SECRET=your_kraken_api_secret_here

# MEXC API Credentials
# Obtain from: https://www.mexc.com/user/openapi
# Note: API secret is plain text (not base64-encoded)
MEXC_API_KEY=your_mexc_api_key_here
MEXC_API_SECRET=your_mexc_api_secret_here

# Optional: Logging level (trace, debug, info, warn, error)
RUST_LOG=info
